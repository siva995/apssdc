<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<version>0.0.1-SNAPSHOT</version>
	<groupId>com.apssdc</groupId>
	<artifactId>sdc</artifactId>

	<properties>
		<spring.version>4.3.1.RELEASE</spring.version>
		<spring.security.version>3.2.9.RELEASE</spring.security.version>
		<apache.tiles.version>3.0.5</apache.tiles.version>
		<org.slf4j.version>1.5.10</org.slf4j.version>
		<javax.jstl.version>1.2</javax.jstl.version>
		<jackson.version>1.9.10</jackson.version>
		<validation.api.version>1.1.0.Final</validation.api.version>
		<hibernate.version>4.1.0.Final</hibernate.version>
		<hibernate.validator.version>5.2.4.Final</hibernate.validator.version>
		<hibernate.annotation.version>5.0.1.Final</hibernate.annotation.version>
		<mysql.connector>5.1.30</mysql.connector>
		<log4j.version>1.2.14</log4j.version>
		<c3p0.version>0.9.2.1</c3p0.version>
		<joda.time.version>2.3</joda.time.version>
		<java.mail.version>1.4.5</java.mail.version>
		<jackson.core>2.5.0</jackson.core>
	</properties>


	<build>
		<!-- <finalName>sdc</finalName> -->
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
					<!-- <compilerArgument>-Xlint:all</compilerArgument> -->
					<compilerArgs>
						<!-- <arg>-Xmaxerrs=1000</arg> -->
						<arg>-Xlint</arg>
					</compilerArgs>
					<showWarnings>true</showWarnings>
					<showDeprecation>true</showDeprecation>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<version>2.7</version>
			</plugin>
		</plugins>
	</build>

